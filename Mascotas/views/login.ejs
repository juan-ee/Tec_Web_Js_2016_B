<div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4">
        <form action="/Auth/login">
            <h1>LOGIN</h1>
            <div class="form-group">
                <label>Correo</label>
                <% if(correo==undefined){%>
                <input type="email" class="form-control" placeholder="Ingrese su correo" name="correo">
                <%}else{%>
              <input type="email" class="form-control" placeholder="Ingrese su correo" name="correo" value="<%= correo %>">
                <%}%>
            </div>
            <div class="form-group">
                <label>Password</label>
                <input type="password" class="form-control" placeholder="Passqord" name="password">
            </div>
            <button type="submit" class="btn btn-block btn-info">Ingresar</button>
        </form>
    </div>
</div>
<div class="col-sm-4"></div>
</div>
