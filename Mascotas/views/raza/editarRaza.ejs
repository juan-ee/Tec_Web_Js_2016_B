


<h1>Editar Raza <%=razaAEditar.nombre%></h1>

<div class="row">
  <div class="col-sm-5">
      <h1>AHORA</h1>
    <form action="/raza/editarRaza" method="post">
      <div class="form-group">
        <label>Nombres</label>
        <input type="text" class="form-control" placeholder="Ingrese sus nombres" name="nombre" value="<%=razaAEditar.nombre%>">
      </div>
      <input type="hidden" name="id" value="<%= razaAEditar.id %>">
      <button type="submit" class="btn btn-info btn-block">Editar Raza</button>
    </form>
  </div>
  <div class="col-sm-2"></div>
  <div class="col-sm-5">
    <h1>ANTES</h1>
    <h2 style="color: #0d3349"><%=razaAEditar.id%></h2>
    <h2 style="color: #0d3349"><%=razaAEditar.nombre%></h2>
  </div>
</div>
